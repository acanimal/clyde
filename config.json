{

  "_prefilters" : "Global filters applied before any provider",
  "prefilters" : [    
    {
      "id" : "logger",
      "path" : "./simple-access-log",
      "config" : {
        "directory" : "./tmp/log",
        "file" : "access-pre-%DATE%.log"
      }
    }
  ],


  "_providers" : "Routes to our private APIs",
  "providers" : [
    {      
      "id" : "providerA",
      "context" : "/providerA",
      "target" : "http://localhost:8890",
      "prefilters" : [
        {
          "id" : "cors",
          "path" : "./filters/cors"
        }
      ]
    },
    {
      "id" : "providerB",
      "context" : "/providerB",      
      "target" : "http://localhost:8891",
      "prefilters" : [],
      "postfilters" : []
    }
  ],


  "_postfilters" : "Global filters applied after any provider",
  "postfilters" : [
    
  ]

}